---
export const prerender = true
import ChampionPage from "@layouts/ChampionPage.astro"
import { capitalizeChampionName } from "@utils"

let { name } = Astro.params
name = capitalizeChampionName(name as string)
const response = await fetch(`http://ddragon.leagueoflegends.com/cdn/13.1.1/data/en_US/champion/${name}.json`)
const data = await response.json()
---

<ChampionPage champion={data.data[name]} title={"aaaaaa"} />
