---
export const prerender = true
import ChampionPage from "@layouts/ChampionPage.astro"

let { id } = Astro.params
const response = await fetch(`http://ddragon.leagueoflegends.com/cdn/13.1.1/data/en_US/champion/${id}.json`)
const data = await response.json()
---

<ChampionPage champion={data.data[id as string]} />
